<!doctype html>
 
<html class="no-js" lang="en">
 
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Nuevos usuarios || Arte stelen</title>
    <meta name="description" content="Sufee Admin - HTML5 Admin Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="apple-touch-icon" href="apple-icon.png">
    <link rel="shortcut icon" href="favicon.ico">


    <link rel="stylesheet" href="/da/vendors/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="/da/vendors/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="/da/vendors/themify-icons/css/themify-icons.css">
    <link rel="stylesheet" href="/da/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="/da/vendors/selectFX/css/cs-skin-elastic.css">
    <link rel="stylesheet" href="/da/vendors/datatables.net-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="/da/vendors/datatables.net-buttons-bs4/css/buttons.bootstrap4.min.css">

    <link rel="stylesheet" href="/da/assets/css/style.css">

    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800' rel='stylesheet' type='text/css'>
</head>

<body>
     

    <!-- Right Panel -->

    <div id="right-panel" class="right-panel">

        <!-- Header-->

        <!-- Header-->

        <div class="breadcrumbs">
            <div class="col-sm-4">
                <div class="page-header float-left">
                    <div class="page-title">
                        <h1>Nuevos usuarios</h1>
                    </div>
                </div>
            </div>
            <div class="col-sm-8">
                <div class="page-header float-right">
                    <div class="page-title">
                       
                    </div>
                </div>
            </div>
        </div>

        <div class="content mt-3">
            <div class="animated fadeIn">
                <div class="row">

                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <strong class="card-title">Todas de cuentas registradas:
                                </strong>
                            </div>
                            <div class="card-body">
                                <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Nombre</th>
                                            <th>Email</th>                                             
                                        </tr>
                                    </thead>
                                    <% users.forEach(function (user) { %>
                                    <tbody>
                                        <tr>
                                            <td><%= user.name %></td>
                                            <td><%= user.email %></td>                                        
                                        </tr>
                                    </tbody>
                                    <% }) %>            
                                </table>
                            </div>
                        </div>
                    </div>


                </div>
            </div><!-- .animated -->
        </div><!-- .content -->


    </div><!-- /#right-panel -->

    <!-- Right Panel -->




    <script src="da/vendors/jquery/dist/jquery.min.js"></script>
    <script src="da/vendors/popper.js/dist/umd/popper.min.js"></script>
    <script src="da/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="da/assets/js/main.js"></script>


    <script src="da/vendors/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="da/vendors/datatables.net-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="da/vendors/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="da/vendors/datatables.net-buttons-bs4/js/buttons.bootstrap4.min.js"></script>
    <script src="da/vendors/jszip/dist/jszip.min.js"></script>
    <script src="da/vendors/pdfmake/build/pdfmake.min.js"></script>
    <script src="da/vendors/pdfmake/build/vfs_fonts.js"></script>
    <script src="da/vendors/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="da/vendors/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="da/vendors/datatables.net-buttons/js/buttons.colVis.min.js"></script>
    <script src="da/assets/js/init-scripts/data-table/datatables-init.js"></script>


</body>

</html>
